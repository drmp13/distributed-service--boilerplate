    "jsonwebtoken": "^8.5.1",
    "dotenv": "^16.0.0",
    "fastify": "^3.27.2",
    "fastify-auth": "^1.1.0",
    "fastify-cors": "^6.0.3",
    "fastify-helmet": "^7.0.1",
    "fastify-jwt": "^4.1.3",
    "fastify-swagger": "^5.0.0",
